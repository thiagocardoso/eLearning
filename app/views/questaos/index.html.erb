<h1>Listing questaos</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Descricao</th>
    <th>Corpo</th>
  </tr>

<% @questaos.each do |questao| %>
  <tr>
    <td><%=h questao.nome %></td>
    <td><%=h questao.descricao %></td>
    <td><%=h questao.corpo %></td>
    <td><%= link_to 'Show', questao %></td>
    <td><%= link_to 'Edit', edit_questao_path(questao) %></td>
    <td><%= link_to 'Destroy', questao, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New questao', new_questao_path %>